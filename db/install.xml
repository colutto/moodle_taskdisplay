<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/taskdisplay/db" VERSION="20210407" COMMENT="Database to test the functionality."
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_taskdisplay" COMMENT="Saves occuring events when user gets graded.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="user_id" TYPE="int" LENGTH="10" COMMENT="The user id" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="course_id" TYPE="int" LENGTH="10" COMMENT="The course id" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>